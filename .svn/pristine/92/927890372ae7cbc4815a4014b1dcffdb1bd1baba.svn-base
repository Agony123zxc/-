                            pNumber:$("#totalNum").val(),//数量
                            pPatentApplicationNumber:$("#zlhApply").val(),//专利申请号
                            pPatentNumber:$("#zlh").val(),//专利号
                            pPhotoAnnex:dispic,//图片
                            pRegistrationAuthority:$("#zcdjjg").val(),//注册登记机关
                            pRegistrationTime:$("#zcdjrq").val(),//注册登记日期
