                            pReserveField:$("#spare").val(),//备用字段
                            pTotalDepreciation:$("#totalZj").val(),//累计折旧
                            pTypeName:$("#number").val(),//资产分类名称
                            pUsestatus:$("#useState").val(),//使用状况
                            pValue:$("#totalPrc").val(),//价值
