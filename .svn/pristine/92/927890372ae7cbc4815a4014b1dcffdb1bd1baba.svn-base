// 工具栏按钮事件
var ButtonInit = function() {
    var oInit = new Object();
    var postdata = {};
    var formData = new FormData();
    var upLoadImg = '';
    var dispic;
    oInit.Init = function() {
        // 添加表格数据
        $("#btn_add").on('click', function() {

            layer.open({
                type: 1,
                title: '添加',
                area: ['760px', '500px'],
                btn: ['确定', '取消'],
                yes: function(index, layero) {
                    var work_number = layero.find('#work_number').val(); //工号
                    if($("#zcName").val()==""){
                        layer.msg("请输入资产名称")
                        return;
                    }
                    if($("#number").val()==""){
                        layer.msg("请输入资产分类名称")
                        return;
                    }
                    // if($("select[name='getType']").val()==""){
                    if ($("select[name='getType']").val() == "") {
                        layer.msg("请选择取得方式")
                        return;
                    }
                    if($("#manageDepart").val()==""){
                        layer.msg("请选择管理部门")
                        return;
                    }
                    if($("#quDate").val()==""){
                        layer.msg("请选择取得日期")
                        return;
                    }
                    if($("#totalNum").val()==""){
                        layer.msg("请输入数量")
                        return;
                    }
                    if($("#kjpzh").val()==""){
                        layer.msg("请输入会计凭证号")
                        return;
                    }

                    $.ajax({
                        url:port+"/v1/patent/addPatent",
