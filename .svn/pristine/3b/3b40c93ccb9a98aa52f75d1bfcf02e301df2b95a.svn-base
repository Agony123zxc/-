                        $.ajax({
                            url: port + '/v1/unPatentedTechnology/deleteUnPatentedTechnology',
                            type: 'post',
                            data: {
                                ids: delData.toString()
                            },
                            success: function(data) {
                                console.log(data)
                                $("#tb_departments").bootstrapTable('refresh'); //刷新表格数据
                                // $('#tree').treeview({ 
                                //     data: getTree()
                                // });
                                layer.msg('删除成功');
                                setTimeout(function() {
